<!DOCTYPE html>
@{ 
    var name = "Robert Paulson";
    if (!string.IsNullOrEmpty(Request["name"]))
    {
        name = Request["name"];
    }
    if (Request["rule"] != null)
    {
        List<String> rules;
        if (Session["rules"] != null){
            rules = (List<String>)Session["rules"];
        }
        else{
            rules = new List<String>();
        }
        rules.Add(Request["rule"]);
        Session["rules"] = rules;
        Response.Redirect("Rules.cshtml");
    }
}
<html>
<head>
    <title></title>
    
</head>
<body>
    <ul>
        <li>btw i am working</li>
        <li>@Request.QueryString["name"]</li>
        <li>@Request["name"]</li>
    </ul>
    <ul>
        @for (int i = 0; i < 10; i++)
        {
            <li>His name is @name</li>
        }
    </ul>

</body>
</html>
